{
    "name": "GiphyNameIs",
    "description": "Giphy 이름을 알려주는 확장 도구입니다.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "main.html"
    },
    "background": {
        "service_worker": "apikey.js"
    },
    "content_scripts": [
        {
            "matches": ["https://www.instagram.com/*"],
            "js": ["main.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "storage"
    ]
}